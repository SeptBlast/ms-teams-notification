{
    "name": "typescript-action",
    "version": "0.0.0",
    "private": true,
    "description": "TypeScript template action",
    "main": "lib/main.js",
    "scripts": {
        "build": "tsc",
        "format": "prettier --write **/*.ts",
        "format-check": "prettier --check **/*.ts",
        "pack": "ncc build",
        "set-node-options": "export NODE_OPTIONS=--openssl-legacy-provider",
        "all": "npm run set-node-options && npm run build && npm run format && npm run pack"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/actions/typescript-action.git"
    },
    "keywords": [
        "actions",
        "node",
        "setup"
    ],
    "author": "septblast",
    "license": "MIT",
    "dependencies": {
        "@actions/core": "^1.2.6",
        "@octokit/graphql": "^4.5.8",
        "@octokit/rest": "^17.11.2",
        "axios": "^1.6.7",
        "moment-timezone": "^0.5.45",
        "octokit": "^3.1.2"
    },
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "@types/moment-timezone": "^0.5.30",
        "@types/node": "^20.11.19",
        "@typescript-eslint/parser": "^7.0.1",
        "@vercel/ncc": "^0.38.1",
        "eslint": "^8.56.0",
        "eslint-plugin-github": "^4.10.1",
        "eslint-plugin-jest": "^27.6.3",
        "jest": "^29.7.0",
        "jest-circus": "^24.9.0",
        "js-yaml": "^3.14.1",
        "prettier": "^1.19.1",
        "ts-jest": "^29.1.2",
        "typescript": "^5.3.3"
    }
}